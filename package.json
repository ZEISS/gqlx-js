{
  "name": "gqlx-js",
  "description": "GraphQL eXtended language and tools library for Node.js / JS applications.",
  "version": "0.2.4",
  "main": "dist",
  "types": "dist",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/example.ts",
    "test:unit": "jest src",
    "test": "npm run lint && npm run test:unit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.ts'",
    "bundle": "browserify dist/bundle.js -o dist/bundle.js -s 'gqlx'",
    "compile": "tsc --project tsconfig.json",
    "minify": "uglifyjs dist/bundle.js > dist/bundle.min.js",
    "build": "npm run compile && npm run bundle && npm run minify"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "graphql-extended <gqlx@anglevisions.com>",
  "browser": {
    "fs": false
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.5",
    "@types/acorn": "^4.0.5",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "graphql": "^14.0.2",
    "jest": "^24.3.1",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.9"
  },
  "peerDependencies": {
    "graphql": "14.x || 0.13.x || 0.12.x || 0.11.x || 0.10.x"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-walk": "^6.1.1"
  }
}
